             x:Class="MauiApp1.Pages.ContentPageDemo.ContentPage3"
             Title="ContentPage3">
    <VerticalStackLayout>
        <Label 
            Text="This is pages 3"
            VerticalOptions="Center" 
            FontSize="25"
            FontAttributes="Bold"
